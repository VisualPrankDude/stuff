

__section_get_fini() {
bin=`which $1`
verbose=""
if test $verbose
then
printf ".fini (dtors) of $bin\n"
fi
r2 -qc "af;S~.fini[5]$" $bin
}

__section_get_init() {
bin=`which $1`
verbose=""
if test $verbose
then
printf ".init (ctors) of $1\n"
fi
r2 -qc "af;S~.init[5]$" $1
}


__getfaddr() {
BIN=$1

if test -z $BIN
then
  echo usage: "function binary"
exit 1
fi

/usr/local/bin/r2 -qc  "f~sym.${BIN}[0]" $2
}
